{
  "$schema": "zed://schemas/settings",
  "auto_install_extensions": {
    "material-icon-theme": true,
    "quiet-light-theme": true,
    "html": true,
    "toml": true,
    "php": true,
    "ruby": true,
    "r": true,
    "sql": true,
    "xml": true,
    "zig": true,
    "log": true,
    "dockerfile": true,
    "docker-compose": true,
    "basher": true,
    "git-firefly": true,
    "env": true,
    "fish": true,
    "nginx": true,
    "mermaid": true,
    "tsgo": true,
    "biome": true,
    "oxc": true,
    "dprint": true,
    "typos": true,
    "factory-droid": true
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "bottom_dock_layout": "contained",
  "title_bar": {
    "show_menus": true,
    "show_branch_icon": true
  },
  "go_to_definition_fallback": "none",
  "toolbar": {
    "code_actions": true
  },
  "scroll_beyond_last_line": "off",
  "auto_update": false,
  "session": {
    "restore_unsaved_buffers": false
  },
  "icon_theme": "Material Icon Theme",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "git": {
    "inline_blame": {
      "delay_ms": 0,
      "show_commit_summary": true,
      "enabled": false
    }
  },
  "base_keymap": "VSCode",
  "theme": "Quiet Light",
  "preview_tabs": {
    "enabled": true,
    "enable_preview_from_file_finder": true,
    "enable_keep_preview_on_code_navigation": true
  },
  "tab_bar": {
    "show": true
  },
  "tabs": {
    "file_icons": true,
    "show_diagnostics": "off",
    "show_close_button": "hover",
    "git_status": true
  },
  "show_signature_help_after_edits": true,
  "auto_signature_help": true,
  "multi_cursor_modifier": "alt",
  "sticky_scroll": {
    "enabled": false
  },
  "gutter": {
    "line_numbers": true,
    "folds": true
  },
  "minimap": {
    "show": "never",
    "thumb": "always",
    "max_width_columns": 80
  },
  "selection_highlight": true,
  "current_line_highlight": "all",
  "cursor_shape": "bar",
  "cursor_blink": true,
  "use_system_window_tabs": true,
  "max_tabs": 10,
  "use_system_path_prompts": true,
  "restore_on_file_reopen": true,
  "restore_on_startup": "last_session",
  "autosave": "on_window_change",
  "confirm_quit": true,
  "text_rendering_mode": "platform_default",
  "ui_font_size": 16.0,
  "buffer_font_size": 14.0,
  "buffer_font_fallbacks": ["Fira Code", "Monaco"],
  "buffer_font_family": "JetBrains Mono NL",
  "file_types": {
    "TypeScript": ["*.ts"],
    "JavaScript": ["*.js"],
    "Git Modules": [".gitmodules"],
    "JSONC": ["*.sublime-settings", ".prettierrc"],
    "Git Attributes": ["**/{git,.git,.git/info}/attributes"],
    "Git Config": [
      "*.gitconfig",
      "**/{git,.git/modules,.git/modules/*}/config"
    ],
    "Git Ignore": ["**/{git,.git}/ignore", "**/.git/info/exclude"]
  },
  "colorize_brackets": true,
  "show_completions_on_input": true,
  "auto_indent_on_paste": false,
  "linked_edits": true,
  "use_on_type_format": false,
  "format_on_save": "on",
  "show_whitespaces": "all",
  "show_edit_predictions": false,
  "ensure_final_newline_on_save": false,
  "indent_guides": {
    "background_coloring": "indent_aware",
    "coloring": "indent_aware",
    "enabled": true
  },
  "wrap_guides": [80, 100, 120],
  "preferred_line_length": 80,
  "soft_wrap": "preferred_line_length",
  "hard_tabs": false,
  "tab_size": 2,
  "prettier": {
    "allowed": false
  },
  "git_panel": {
    "tree_view": true,
    "dock": "left",
    "collapse_untracked_diff": false,
    "status_style": "label_color",
    "sort_by_path": true,
    "button": true,
    "default_width": 320
  },
  "project_panel": {
    "hide_gitignore": false,
    "git_status": true,
    "auto_reveal_entries": true,
    "auto_fold_dirs": true,
    "button": true,
    "default_width": 320
  },
  "outline_panel": {
    "dock": "right",
    "auto_fold_dirs": true,
    "git_status": true,
    "file_icons": true,
    "folder_icons": true,
    "expand_outlines_with_depth": 100,
    "auto_reveal_entries": false,
    "button": true,
    "default_width": 320
  },
  "notification_panel": {
    "dock": "right",
    "button": true,
    "default_width": 360
  },
  "terminal": {
    "line_height": "standard",
    "option_as_meta": false,
    "minimum_contrast": 0,
    "font_size": 14.0,
    "font_family": "JetBrains Mono",
    "font_fallbacks": ["Fira Code", "Monaco"],
    "cursor_shape": "block",
    "blinking": "on",
    "copy_on_select": false,
    "max_scroll_history_lines": 10000,
    "shell": "system",
    "detect_venv": {
      "on": {
        "directories": [".env", "env", ".venv", "venv"],
        "activate_script": "default"
      }
    },
    "toolbar": {
      "breadcrumbs": false
    },
    "scroll_multiplier": 4,
    "button": true,
    "default_height": 420
  },
  "disable_ai": false,
  "language_models": {
