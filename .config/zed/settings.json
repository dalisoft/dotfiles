  },
  "agent": {
    "agents_panel_dock": "right",
    "enabled": true,
    "default_model": {
      "model": "claude-opus-4-5(0)",
      "provider": "CLIProxyAPI"
    },
    "inline_assistant_model": {
      "model": "claude-sonnet-4-5(0)",
      "provider": "cliproxyapi"
    },
    "inline_alternatives": [
      {
        "model": "gemini-3-flash-preview(minimal)",
        "provider": "cliproxyapi"
      }
    ],
    "default_profile": "minimal",
    "default_view": "thread",
    "always_allow_tool_actions": true,
    "button": true,
    "default_width": 540
  },
  "agent_servers": {
    "gemini": {
      "ignore_system_version": false
    }
  },
  "lsp": {
    "typos": {
      "initialization_options": {
        "diagnosticSeverity": "Hint",
        "logLevel": "info"
      }
    },
    "tsgo": {
      "settings": {
        "goMemLimit": "4GiB",
        "trace": { "server": "off" }
      }
    },
    "vtsls": {
      "settings": {
        "vtsls": {
          "autoUseWorkspaceTsdk": true,
          "experimental": {
            "completion": {
              "enableServerSideFuzzyMatch": true,
              "entriesLimit": 10
            }
          }
        },
        "typescript": {
          "preferGoToSourceDefinition": true,
          "tsserver": {
            "useSyntaxServer": "never",
            "maxTsServerMemory": 4096,
            "enableTracing": false,
            "log": "off"
          }
        },
        "javascript": {
          "preferGoToSourceDefinition": true,
          "tsserver": {
            "useSyntaxServer": "never",
            "maxTsServerMemory": 4096,
            "enableTracing": false,
            "log": "off"
          }
        }
      }
    }
  },
  "languages": {
    "Markdown": {
      "prettier": { "allowed": true },
      "formatter": "prettier"
    },
    "JSON": {
      "formatter": {
        "language_server": { "name": "biome" }
      }
    },
    "JSONC": {
      "formatter": {
        "language_server": { "name": "biome" }
      }
    },
    "JavaScript": {
      "language_servers": ["tsgo"],
      "formatter": {
        "language_server": { "name": "biome" }
      }
    },
    "TypeScript": {
      "language_servers": ["tsgo"],
      "formatter": {
        "language_server": { "name": "biome" }
      }
    }
  }
}
